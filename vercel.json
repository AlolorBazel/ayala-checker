{
    "rewrites": [
        {
            "source": "/",
            "destination": "/home.html"
        },
        {
            "source": "/home",
            "destination": "/home.html"
        },
        {
            "source": "/arenata",
            "destination": "/arenata.html"
        },
        {
            "source": "/ayala",
            "destination": "/ayala.html"
        }
    ],
        
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}